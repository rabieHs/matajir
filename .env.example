# Stripe Configuration
# Copy this file to .env and replace with your actual Stripe keys

# Test/Sandbox Keys (get from https://dashboard.stripe.com/ in test mode)
STRIPE_TEST_PUBLISHABLE_KEY=pk_test_YOUR_TEST_PUBLISHABLE_KEY_HERE
STRIPE_TEST_SECRET_KEY=sk_test_YOUR_TEST_SECRET_KEY_HERE

# Live/Production Keys (get from https://dashboard.stripe.com/ in live mode)
STRIPE_LIVE_PUBLISHABLE_KEY=pk_live_YOUR_LIVE_PUBLISHABLE_KEY_HERE
STRIPE_LIVE_SECRET_KEY=sk_live_YOUR_LIVE_SECRET_KEY_HERE

# Environment setting (true for production, false for test)
STRIPE_IS_PRODUCTION=false
